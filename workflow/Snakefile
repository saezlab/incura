from snakemake.utils import min_version

min_version('7.14.0')

configfile: 'config/config.yaml'
#container: 'workflow/envs/incura.sif'

ORGANISM = config['organism']

rule all:
    input: 
        'data/fimo/fimo.tsv'

#------------------------------------------------------------------------------------
module runFIMO:
    snakefile: "rules/runFIMO.smk"
    config: config

use rule runFIMO from runFIMO


